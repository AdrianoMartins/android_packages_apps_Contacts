    <string name="date_qs_currentquarter">Trimestre Atual</string>
    <string name="date_qs_currentyear">Ano Corrente</string>
    <string name="date_qs_lastweek">Semana Passada</string>
    <string name="date_qs_lastmonth">Mês Passada</string>
    <string name="date_qs_lastquarter">Último Trimeste</string>
    <string name="date_qs_lastyear">Ano Passado</string>

    <string name="call_stats_date_filter">Ajustar o intervalo de tempo</string>
    <string name="call_stats_reset_filter">Redefinir o intervalo de tempo</string>
    <string name="call_stats_sort_by_duration">Ordenar pela duração da chamada</string>
    <string name="call_stats_sort_by_count">Ordenar pela contagem de chamadas</string>

    <!-- 0:           sec
         1:      min
         2:      min  sec
         3: hour
         4: hour      sec
         5: hour min
         6: hour min  sec -->

    <string-array name="call_stats_duration">
        <item><xliff:g id="seconds" example="2 segundos">%3$s</xliff:g></item>
        <item><xliff:g id="minutes" example="2 minutos">%2$s</xliff:g></item>
        <item><xliff:g id="minutes" example="2 minutos">%2$s</xliff:g> <xliff:g id="seconds" example="2 segundos">%3$s</xliff:g></item>
        <item><xliff:g id="hours" example="2 horas">%1$s</xliff:g></item>
        <item><xliff:g id="hours" example="2 horas">%1$s</xliff:g> <xliff:g id="seconds" example="2 segundos">%3$s</xliff:g></item>
        <item><xliff:g id="hours" example="2 horas">%1$s</xliff:g> <xliff:g id="minutes" example="2 minutos">%2$s</xliff:g></item>
        <item><xliff:g id="hours" example="2 horas">%1$s</xliff:g> <xliff:g id="minutes" example="2 minutos">%2$s</xliff:g> <xliff:g id="seconds" example="2 segundos">%3$s</xliff:g></item>
    </string-array>

    <plurals name="hour">
        <item quantity="one">1 hr</item>
        <item quantity="other">%d hrs</item>
    </plurals>
    <plurals name="minute">
        <item quantity="one">1 min</item>
        <item quantity="other">%d mins</item>
    </plurals>
    <plurals name="second">
        <item quantity="one">1 seg</item>
        <item quantity="other">%d segs</item>
    </plurals>

    <plurals name="call">
        <item quantity="one">1 chamada</item>
        <item quantity="other">%d chamadas</item>
    </plurals>

    <string name="menu_add_to_blacklist">Adicionar a Lista Negra</string>
    <string name="toast_added_to_blacklist">%s adicionado a Lista Negra.</string>

</resources>
