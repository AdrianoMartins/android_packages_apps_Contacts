<resources xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- Title bar for call stat detail screen -->
    <string name="callStatsDetailTitle">Estatística de Chamadas</string>

    <string name="call_stats">Estatísticas</string>
    <string name="call_stats_refresh">Atualizar</string>

    <string name="activity_title_call_stats">Estatística de Chamadas</string>

    <string name="call_stats_nav_all">Todas</string>
    <string name="call_stats_nav_incoming">Recebidas</string>
    <string name="call_stats_nav_outgoing">Efetuadas</string>
    <string name="call_stats_nav_missed">Perdidas</string>
    <string name="call_stats_incoming">Recebida: <xliff:g id="percent">%d</xliff:g>%%</string>
    <string name="call_stats_outgoing">Efetuada: <xliff:g id="percent">%d</xliff:g>%%</string>
    <string name="call_stats_missed">Perdidas</string>
    <string name="call_stats_missed_percent">Perdida: <xliff:g id="percent">%d</xliff:g>%%</string>
    <string name="call_stats_header_total">Total: <xliff:g id="call_count">%s</xliff:g>, <xliff:g id="duration">%s</xliff:g></string>
    <string name="call_stats_header_total_callsonly">Total: <xliff:g id="call_count">%s</xliff:g></string>
    <string name="call_stats_filter_from">Data Inicial</string>
    <string name="call_stats_filter_to">Data Final</string>
    <string name="call_stats_filter_picker_title">Filtro de Intervalo</string>

    <string name="date_quick_selection">Seleção Rápida</string>
